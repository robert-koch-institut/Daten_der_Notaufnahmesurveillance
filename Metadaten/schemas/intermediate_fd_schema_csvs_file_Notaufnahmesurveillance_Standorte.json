{
  "fields": [
    {
      "name": "ik_number",
      "type": "string",
      "constraints": {
        "pattern": "[0-9]*"
      }
    },
    {
      "name": "ed_name",
      "type": "string",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "ed_type",
      "type": "string",
      "constraints": {
        "enum": [
          "central",
          "pediatric"
        ]
      }
    },
    {
      "name": "level_of_care",
      "type": "string",
      "constraints": {
        "enum": [
          "Basisnotfallversorgung",
          "Erweiterte Versorgung",
          "Umfassende Versorgung"
        ]
      }
    },
    {
      "name": "state",
      "type": "string",
      "constraints": {
        "enum": [
          "Baden-Württemberg",
          "Bayern",
          "Berlin",
          "Brandenburg",
          "Bremen",
          "Hamburg",
          "Hessen",
          "Mecklenburg-Vorpommern",
          "Niedersachsen",
          "Nordrhein-Westfalen",
          "Rheinland-Pfalz",
          "Saarland",
          "Sachsen",
          "Sachsen-Anhalt",
          "Schleswig-Holstein",
          "Thüringen"
        ]
      }
    },
    {
      "name": "state_id",
      "type": "string",
      "constraints": {
        "pattern": "0[1-9]{1}|1[0-6]{1}"
      }
    },
    {
      "name": "latitude",
      "type": "number",
      "constraints": {
        "minimum": 0
      }
    },
    {
      "name": "longitude",
      "type": "number",
      "constraints": {
        "minimum": 0
      }
    }
  ]
}